<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper   
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"   
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
    
<mapper namespace="kr.watchu.movie.dao.BoardMapper">
	<select id="selectMovieList" parameterType="Map" resultType="MovieCommand">
	SELECT * FROM
	(SELECT a.*,rownum rnum FROM
	(SELECT * FROM movie_info ORDER BY title desc)a)
	<![CDATA[
		WHERE rnum = >= #{start} AND rnum <= #{end}
	]]>	
	</select>
	
	<select id="selectMovieCnt" resultType="Integer">
		SELECT count(*) FROM movie_info
	</select>
</mapper>